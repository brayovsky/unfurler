{
  "name": "unfurler",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "migrate": "npx -n '-r dotenv/config' migrate",
    "start": "npm run migrate && node -r dotenv/config index.js",
    "start:dev": "nodemon -r dotenv/config index.js"
  },
  "dependencies": {
    "@google-cloud/translate": "^6.0.2",
    "connect-busboy": "0.0.2",
    "cross-fetch": "^3.0.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gridfs-stream": "^1.1.1",
    "htmlparser2": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "md5": "^2.2.1",
    "mongodb": "^3.5.9",
    "uniqid": "^5.2.0"
  },
  "devDependencies": {
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "migrate": "^1.7.0",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
